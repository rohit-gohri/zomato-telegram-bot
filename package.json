{
  "name": "telegram-bot-test",
  "version": "0.2.0",
  "description": "Zomato telegram bot",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "start": "yarn compile && node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\" --onFailure \"echo Beep! Compilation Failed\" --compiler $(which tsc)",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohit-gohri/telegram-bot-test.git"
  },
  "keywords": [
    "telegram",
    "zomato",
    "bot",
    "search"
  ],
  "author": "Rohit Gohri",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/rohit-gohri/telegram-bot-test/issues"
  },
  "homepage": "https://github.com/rohit-gohri/telegram-bot-test#readme",
  "dependencies": {
    "@smpx/oak": "^1.2.7",
    "lodash": "^4.17.11",
    "postinstall-postinstall": "^2.0.0",
    "sm-utils": "^2.17.21",
    "telegraf": "^3.32.0",
    "universal-analytics": "^0.4.20"
  },
  "devDependencies": {
    "@now/node": "^1.0.1",
    "@types/lodash": "^4.14.120",
    "@types/node": "^12.7.12",
    "@types/universal-analytics": "^0.4.2",
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "@typescript-eslint/parser": "^1.1.0",
    "eslint": "^5.12.1",
    "jest": "^24.0.0",
    "patch-package": "^6.2.0",
    "tsc-watch": "^1.1.34",
    "typescript": "^3.2.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": "plugin:@typescript-eslint/recommended"
  },
  "resolutions": {
    "sm-utils": "2.17.21",
    "@types/node": "12.7.12",
    "@types/ioredis": "4.0.18"
  }
}
